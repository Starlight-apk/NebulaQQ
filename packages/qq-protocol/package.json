  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "crypto-js": "^4.2.0",
    "pako": "^2.1.0",
    "protobufjs": "^7.2.6",
    "ws": "^8.16.0",
    "axios": "^1.6.0",
    "qrcode": "^1.5.3"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/crypto-js": "^4.2.0",
    "@types/pako": "^2.0.3",
    "@types/ws": "^8.5.10",
    "typescript": "^5.3.0"
