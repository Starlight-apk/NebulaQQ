{
  "name": "@nebulaqq/module-sdk",
  "version": "1.0.0",
  "description": "NebulaQQ 模块 SDK - 模块开发工具包",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "clean": "rm -rf dist",
    "prepublishOnly": "npm run build"
  },
  "os": [
    "linux",
    "darwin",
    "win32"
  ],
  "cpu": [
    "x64",
    "arm64",
    "arm"
  ],
  "engines": {
    "node": ">=18.0.0"
  },
  "keywords": [
    "nebulaqq",
    "module",
    "sdk",
    "termux",
    "arm64"
  ],
  "author": "NebulaQQ Team",
  "license": "MIT",
  "dependencies": {
    "@nebulaqq/core": "workspace:*"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "typescript": "^5.3.0"
  }
}
